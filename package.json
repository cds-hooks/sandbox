{
  "name": "cds-hooks-sandbox",
  "version": "0.1.0",
  "description": "A SPA tool that simulates the CDS Hooks standard on a mock-ehr system",
  "main": "build/bundle.js",
  "scripts": {
    "start": "node scripts/express/app.js",
    "dev": "webpack-dev-server --config webpack.config.dev.js --inline --content-base ./build",
    "compile:heroku": "webpack --config webpack.config.dev.js --progress --colors --display-error-details",
    "build": "webpack --config webpack.config.dev.js --progress --colors --display-error-details",
    "heroku-postbuild": "npm install --only=dev && npm run compile:heroku",
    "lint": "./node_modules/.bin/eslint ./src --fix",
    "test": "jest --coverage"
  },
  "author": "",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cds-hooks/sandbox.git"
  },
  "bugs": {
    "url": "https://github.com/cds-hooks/sandbox/issues"
  },
  "homepage": "https://github.com/cds-hooks/sandbox#readme",
  "keywords": [
    "cds",
    "hooks",
    "sandbox"
  ],
  "dependencies": {
    "axios": "^0.17.1",
    "classnames": "^2.2.5",
    "jsrsasign": "^8.0.6",
    "lodash": "^4.17.5",
    "query-string": "^5.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "terra-base": "^3.1.0",
    "terra-card": "^2.1.0",
    "terra-form": "^2.1.0",
    "terra-heading": "^2.1.0",
    "terra-overlay": "^2.1.0",
    "terra-toggle": "^2.1.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "axios-mock-adapter": "^1.11.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "node-sass": "^4.7.2",
    "postcss-custom-properties": "^7.0.0",
    "postcss-loader": "^2.1.1",
    "raw-loader": "^0.5.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.4.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "terra-i18n-plugin": "^2.0.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0"
  },
  "jest": {
    "setupFiles": [
      "./tests/jest-setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "coveragePathIgnorePatterns": [
      "/jest-setup.js",
      "/store.js"
    ]
  }
}
